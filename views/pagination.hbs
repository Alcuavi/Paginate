{{#each films}}
    <div class="panel panel-warning">
        <div class="panel-heading">
            <h5>{{id}}</h5>
        </div>
        <div class="panel-body">
            <p>{{description}}</p>
        </div>
        <div class="panel-footer">
            <p>{{title}}</p>
        </div>
    </div>
    {{/each}}