{{#each films}}
    <div class="panel panel-warning">
        <div class="panel-heading">
            <h5>{{id}}</h5>
        </div>
        <div class="panel-body">
            <p>{{description}}</p>
        </div>
        <div class="panel-footer">
            <p>{{title}}</p>
        </div>
    </div>
    {{/each}}

<nav aria-label="Page pagination example">
    <ul class="pagination">
        {{#with paginate}}
            {{if hasPreviusPages}}
                <li class="page-item prev">
                    <a class="page-link" href="{{prev_link this}}">
                        Anterior
                    </a>
                </li>
            {{/if}}
        {{/with}}
    </ul>
</nav>